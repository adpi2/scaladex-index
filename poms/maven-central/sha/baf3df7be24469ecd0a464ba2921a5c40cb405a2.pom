<?xml version='1.0' encoding='UTF-8'?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.slamdata</groupId>
    <artifactId>sbt-slamdata</artifactId>
    <packaging>jar</packaging>
    <description>Common build configuration for SBT projects</description>
    <url>https://github.com/slamdata/sbt-slamdata</url>
    <version>0.5.5</version>
    <licenses>
        <license>
            <name>Apache 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0</url>
            <distribution>repo</distribution>
        </license>
    </licenses>
    <name>sbt-slamdata</name>
    <organization>
        <name>SlamData Inc.</name>
        <url>http://slamdata.com</url>
    </organization>
    <scm>
        <url>https://github.com/slamdata/sbt-slamdata</url>
        <connection>scm:git@github.com:slamdata/sbt-slamdata.git</connection>
    </scm>
    <developers>
        <developer>
            <id>slamdata</id>
            <name>SlamData Inc.</name>
            <email>contact@slamdata.com</email>
            <url>http://slamdata.com</url>
        </developer>
    </developers>
    <properties>
        <scalaVersion>2.12</scalaVersion>
        <sbtVersion>1.0</sbtVersion>
        <extraDependencyAttributes xml:space="preserve">
            +e:sbtVersion:#@#:+1.0:#@#:+module:#@#:+sbt-pgp:#@#:+e:scalaVersion:#@#:+2.12:#@#:+organisation:#@#:+com.jsuereth:#@#:+branch:#@#:+@#:NULL:#@:#@#:+revision:#@#:+1.1.0:#@#:
+e:sbtVersion:#@#:+1.0:#@#:+module:#@#:+sbt-release:#@#:+e:scalaVersion:#@#:+2.12:#@#:+organisation:#@#:+com.github.gseitz:#@#:+branch:#@#:+@#:NULL:#@:#@#:+revision:#@#:+1.0.6:#@#:
+e:sbtVersion:#@#:+1.0:#@#:+module:#@#:+sbt-sonatype:#@#:+e:scalaVersion:#@#:+2.12:#@#:+organisation:#@#:+org.xerial.sbt:#@#:+branch:#@#:+@#:NULL:#@:#@#:+revision:#@#:+2.0:#@#:
+e:sbtVersion:#@#:+1.0:#@#:+module:#@#:+sbt-travisci:#@#:+e:scalaVersion:#@#:+2.12:#@#:+organisation:#@#:+com.dwijnand:#@#:+branch:#@#:+@#:NULL:#@:#@#:+revision:#@#:+1.1.1:#@#:
+e:sbtVersion:#@#:+1.0:#@#:+module:#@#:+sbt-header:#@#:+e:scalaVersion:#@#:+2.12:#@#:+organisation:#@#:+de.heikoseeberger:#@#:+branch:#@#:+@#:NULL:#@:#@#:+revision:#@#:+3.0.2:#@#:
+e:sbtVersion:#@#:+1.0:#@#:+module:#@#:+sbt-wartremover:#@#:+e:scalaVersion:#@#:+2.12:#@#:+organisation:#@#:+org.wartremover:#@#:+branch:#@#:+@#:NULL:#@:#@#:+revision:#@#:+2.2.1:#@#:
        </extraDependencyAttributes>
    </properties>
    <dependencies>
        <dependency>
            <groupId>org.scala-lang</groupId>
            <artifactId>scala-library</artifactId>
            <version>2.12.3</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.scala-sbt</groupId>
            <artifactId>sbt</artifactId>
            <version>1.0.2</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.jsuereth</groupId>
            <artifactId>sbt-pgp</artifactId>
            <version>1.1.0</version>
        </dependency>
        <dependency>
            <groupId>com.github.gseitz</groupId>
            <artifactId>sbt-release</artifactId>
            <version>1.0.6</version>
        </dependency>
        <dependency>
            <groupId>org.xerial.sbt</groupId>
            <artifactId>sbt-sonatype</artifactId>
            <version>2.0</version>
        </dependency>
        <dependency>
            <groupId>com.dwijnand</groupId>
            <artifactId>sbt-travisci</artifactId>
            <version>1.1.1</version>
        </dependency>
        <dependency>
            <groupId>de.heikoseeberger</groupId>
            <artifactId>sbt-header</artifactId>
            <version>3.0.2</version>
        </dependency>
        <dependency>
            <groupId>org.wartremover</groupId>
            <artifactId>sbt-wartremover</artifactId>
            <version>2.2.1</version>
        </dependency>
    </dependencies>
