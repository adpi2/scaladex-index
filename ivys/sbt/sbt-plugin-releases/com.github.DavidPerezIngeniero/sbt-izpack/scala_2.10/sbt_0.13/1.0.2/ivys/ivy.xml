<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:e="http://ant.apache.org/ivy/extra">
	<info organisation="com.github.DavidPerezIngeniero" module="sbt-izpack" revision="1.0.2" status="release" publication="20150325165123" e:scalaVersion="2.10" e:sbtVersion="0.13">
		<license name="BSD New" url="https://github.com/bmc/sbt-izpack/blob/master/LICENSE.md"/>
		<description>
		SBT plugin to generate an IzPack installer
		</description>
	</info>
	<configurations>
		<conf name="compile" visibility="public" description=""/>
		<conf name="runtime" visibility="public" description="" extends="compile"/>
		<conf name="test" visibility="public" description="" extends="runtime"/>
		<conf name="provided" visibility="public" description=""/>
		<conf name="optional" visibility="public" description=""/>
		
		
		
		
		<conf name="sources" visibility="public" description=""/>
		<conf name="docs" visibility="public" description=""/>
		<conf name="pom" visibility="public" description=""/>
		
	</configurations>
	<publications>
		<artifact name="sbt-izpack" type="jar" ext="jar" conf="compile"/>
		<artifact name="sbt-izpack" type="src" ext="jar" conf="sources" e:classifier="sources"/>
		<artifact name="sbt-izpack" type="doc" ext="jar" conf="docs" e:classifier="javadoc"/>
	</publications>
	<dependencies>
		
		<dependency org="org.scala-lang" name="scala-library" rev="2.10.4" conf="provided->default(compile)"/>
		<dependency org="org.scala-sbt" name="sbt" rev="0.13.5" conf="provided->default(compile)"/>
		<dependency org="org.codehaus.izpack" name="izpack-standalone-compiler" rev="4.3.5" conf="compile->default(compile)"/>
		<dependency org="org.yaml" name="snakeyaml" rev="1.14" conf="compile->default(compile)"/>
		<dependency org="org.clapper" name="grizzled-scala_2.10" rev="1.3" conf="compile->default(compile)"/>
	</dependencies>
</ivy-module>
