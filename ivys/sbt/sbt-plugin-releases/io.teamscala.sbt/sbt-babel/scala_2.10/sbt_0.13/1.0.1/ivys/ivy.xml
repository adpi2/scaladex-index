<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:e="http://ant.apache.org/ivy/extra">
	<info organisation="io.teamscala.sbt" module="sbt-babel" revision="1.0.1" status="release" publication="20150730113409" e:sbtVersion="0.13" e:scalaVersion="2.10">
		<license name="MIT" url="http://opensource.org/licenses/MIT"/>
		<description>
		An SBT plugin to perform Babel compilation.
		</description>
	</info>
	<configurations>
		<conf name="compile" visibility="public" description=""/>
		<conf name="runtime" visibility="public" description="" extends="compile"/>
		<conf name="test" visibility="public" description="" extends="runtime"/>
		<conf name="provided" visibility="public" description=""/>
		<conf name="optional" visibility="public" description=""/>
		
		
		
		
		<conf name="sources" visibility="public" description=""/>
		<conf name="docs" visibility="public" description=""/>
		<conf name="pom" visibility="public" description=""/>
		
		
		
	</configurations>
	<publications>
		<artifact name="sbt-babel" type="jar" ext="jar" conf="compile"/>
		<artifact name="sbt-babel" type="src" ext="jar" conf="sources" e:classifier="sources"/>
		<artifact name="sbt-babel" type="doc" ext="jar" conf="docs" e:classifier="javadoc"/>
	</publications>
	<dependencies>
		
		<dependency org="org.scala-lang" name="scala-library" rev="2.10.5" conf="provided->default(compile)"/>
		<dependency org="org.scala-sbt" name="sbt" rev="0.13.8" conf="provided->default(compile)"/>
		<dependency org="com.typesafe" name="jstranspiler" rev="1.0.0" conf="compile->default(compile)"/>
		<dependency org="com.typesafe.sbt" name="sbt-js-engine" rev="1.1.3" conf="compile->default(compile)" e:sbtVersion="0.13" e:scalaVersion="2.10"/>
		
		
		<override org="org.scala-lang" module="scala-library" matcher="exact" rev="2.10.5"/>
		<override org="org.scala-lang" module="scala-compiler" matcher="exact" rev="2.10.5"/>
		<override org="org.scala-lang" module="scala-reflect" matcher="exact" rev="2.10.5"/>
	</dependencies>
</ivy-module>
